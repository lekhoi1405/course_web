<div class="create-course-header">
  <h1 class="create-course-title">Create New Course</h1>
  <div class="create-course-actions">
    <button name="action" value="cancel" class="btn-cancel">Cancel</button>
    <button class="btn-save-course" name="action" value="save">
      <img
        src="/images/addCourse/icon_save.svg"
        alt="Save"
        width="20"
        height="20"
      />
      Save Course
    </button>
    <button class="btn-submit-review" name="action" value="submitReview">
      <img
        src="/images/addCourse/icon_submit.svg"
        alt="Submit"
        width="20"
        height="20"
      />
      Submit for Review
    </button>
  </div>
</div>
